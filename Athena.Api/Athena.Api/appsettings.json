{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "log-{Date}.txt",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      },
      {
        "Name" :  "LiterateConsole" 
      }
    ],
    "Properties": {
      "ApplicationName": "Atena.Api"
    }
  },
  "AthenaConfiguration": {
    "BaseUri": "http://kwestiasmaku.com",
    "RedisHostname": "kisiel"
  } 
}
